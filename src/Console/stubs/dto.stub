<?php

namespace DummyNamespace;

class DummyClass
{
    /**
     * Create a new DTO instance.
     *
     * @param  string  $property
     */
    public function __construct(
        public string $property,
    ) {
    }

    /**
     * Create DTO from array.
     *
     * @param  array  $data
     * @return self
     */
    public static function fromArray(array $data): self
